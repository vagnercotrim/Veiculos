@model NHibernate.Stat.IStatistics

@{
    ViewBag.Title = "Index";
}

<div class="row">

    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                NHibernate
            </div>

            <div class="panel-body">
                <div class="table-responsive">
                    <dl class="dl-horizontal">
                        <dt>
                            Nome dos models
                        </dt>

                        <dd>
                            @foreach (var m in Model.EntityNames)
                            {
                                <p>@m</p>
                            }
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
    </div>

    <div>
        <h4>Veiculo</h4>
        <hr />

    </div>

</div>